<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <_PropertySheetDisplayName>PHL Windows Setting</_PropertySheetDisplayName>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <ClCompile>
      <PreprocessorDefinitions>NDIS_MINIPORT_DRIVER;NDIS60_MINIPORT=1;%(PreprocessorDefinitions)</PreprocessorDefinitions>
	  <PreprocessorDefinitions>$(PHL_PROPS_PLTFM);$(PHL_PROPS_HCI);CONFIG_RTL8852A;CONFIG_RTL8852B;CONFIG_RTL8852C;CONFIG_FSM;CONFIG_PHL_TEST_SUITE;CONFIG_PHL_CUSTOM_FEATURE;CONFIG_PHL_CUSTOM_FEATURE_FB;%(PreprocessorDefinitions)</PreprocessorDefinitions>
    </ClCompile>
  </ItemDefinitionGroup>
</Project>
